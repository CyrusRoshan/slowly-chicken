{
  "manifest_version": 3,
  "name": "Slowly, Chicken",
  "version": "0.0.1",
  "description": "Chicken, but slowly.",
  "icons": {
    "128": "./assets/logo_128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*/"],
      "js": ["chicken.js"],
      "all_frames": true,
      "run_at": "document_idle"
    }
  ]
}
